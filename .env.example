# Copy to .env.local for local development
# These values are set in Coolify for production

# WebSocket URL (production example: wss://ws.yourdomain.com)
NEXT_PUBLIC_WS_URL=ws://localhost:8081

# Session secret (generate with: openssl rand -base64 32)
SESSION_SECRET=your-secret-key-here

# Database Configuration (PostgreSQL) - Required for permanent storage
DATABASE_URL=postgresql://username:password@localhost:5432/chess2ban

# Redis Configuration - Required for game cache
REDIS_URL=redis://localhost:6379


GOOGLE_CLIENT_ID=<from-google>
GOOGLE_CLIENT_SECRET=<from-google>