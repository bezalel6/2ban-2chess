{
  "name": "2ban-2chess",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n \"NEXT,WS\" -c \"cyan,magenta\" \"npm run dev:next\" \"npm run dev:ws\"",
    "dev:next": "portio --mine 3000 && next dev",
    "dev:ws": "portio --mine 8081 && nodemon server/ws-server.ts",
    "build": "next build",
    "start": "next start",
    "ws-server": "portio --mine 8081 && tsx server/ws-server.ts"
  },
  "dependencies": {
    "ban-chess.ts": "^1.1.1",
    "howler": "^2.2.4",
    "next": "14.2.18",
    "react": "^18",
    "react-chessground": "^1.5.0",
    "react-dom": "^18",
    "uuid": "^9.0.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/howler": "^2.2.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-chessground": "^1.5.0",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "tsx": "^4.7.0",
    "typescript": "^5"
  }
}
